<div class="section press shadow-background">
    <div class="container">
        <div class="flex-row">
            <% for ( var i = 0 ; i < 5 ; i++ ) { %>
            <div class="img-container">
            <% if (links[i].link) { %>
            <a target="_blank" href="<%= links[i].link -%>" ><img src="<%= links[i].img -%>"/></a>
            <% } else { %>
            <img src="<%= links[i].img -%>"/>
            <% } %>
            </div>
            <% } %>
        </div>
        <div class="flex-row">
            <% for ( var i = 5 ; i < links.length ; i++ ) { %>
            <div class="img-container">
            <% if (links[i].link) { %>
            <a target="_blank" href="<%= links[i].link -%>" ><img src="<%= links[i].img -%>"/></a>
            <% } else { %>
            <img src="<%= links[i].img -%>"/>
            <% } %>
            </div>
            <% } %>
        </div>
    </div>
    <div class="carousel-wrapper">
        <div class="carousel-frame">
            <ul class="carousel-slides">
                <% for ( var i = 0 ; i < links.length ; i++ ) { %>
                <li>
                  <% if (links[i].link) { %>
                  <a target="_blank" href="<%= links[i].link -%>" ><img src="<%= links[i].img -%>"/></a>
                  <% } else { %>
                  <img src="<%= links[i].img -%>"/>
                  <% } %>
                </li>
                <% } %>
            </ul>
        </div>
    </div>
</div>
